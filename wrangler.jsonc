{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "sink",
  "main": "dist/server/index.mjs",
  "assets": {
    "binding": "ASSETS",
    "directory": "dist/public"
  },
  "compatibility_date": "2025-05-08",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "keep_vars": true,
  "upload_source_maps": true,
  "kv_namespaces": [
    {
      "binding": "Sink",
      "id": "351236ef87c542fbb88d50b835d21564" // IMPORTANT: Change this to your KV namespace ID
    }
  ]
}
